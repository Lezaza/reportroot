<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sunrisedutyfree.com.cdf.report.demorepository.jcpush.POS_REAL_INVRepository">

    <select id="count" resultType="java.lang.Integer">
        select count(*) from POS_REAL_INV
    </select>


    <select id="findPOS_REAL_INVsByXSDATEBetween" resultType="sunrisedutyfree.com.cdf.report.demoentity.jcpush.POS_REAL_INV">
