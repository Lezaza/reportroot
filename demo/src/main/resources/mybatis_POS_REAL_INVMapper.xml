            #{POS_REAL_INV.HYYKXUHAO},#{POS_REAL_INV.JFKXUHAO},#{POS_REAL_INV.GBKXUHAO},#{POS_REAL_INV.KECENG},#{POS_REAL_INV.TRANSFERNO},#{POS_REAL_INV.STORENO},#{POS_REAL_INV.OPFLAG})
